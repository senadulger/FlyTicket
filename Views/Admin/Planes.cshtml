@{
    ViewData["Title"] = "Planes";
}

<div class="container mt-4">
    <h2>Planes</h2>
    <table class="table table-bordered table-hover mt-3">
        <thead class="table-dark">
            <tr>
                <th>ID</th>
                <th>Name</th>
                <th>Seat Count</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var row in Model)
            {
                <tr>
                    <td>@row["id"]</td>
                    <td>@row["name"]</td>
                    <td>@row["seat_count"]</td>
                </tr>
            }
        </tbody>
    </table>
</div>
